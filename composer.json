{
    "name": "translate5/string-encoder",
    "type": "library",
    "description": "A Fork of the paquettg/string-encoder PHP HTML-parser to be able to support newer PHP versions",
    "keywords": ["encoding", "charset", "string", "tranlate5"],
    "license": "MIT",
    "authors": [
        {
            "name": "Axel Becher",
            "email": "axel@mittagqi.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "mockery/mockery": "^1.6.7",
        "phan/phan": "^5.4.0",
        "friendsofphp/php-cs-fixer": "^2.16",
        "php-coveralls/php-coveralls": "^2.4.2"
    },
    "autoload": {
        "psr-4": {
        	"StringEncoder\\": "src/StringEncoder"
       	}
    }
}
